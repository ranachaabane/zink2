.main
{

width: 1000px;
height: 1500px;
margin-top: 200px;
margin-left: 150px;
background-color: white;
border-radius: 10px;
}

.dropdown-content {
  display: none;
  position: absolute;
  background-color: #f9f9f9;
  min-width: 160px;
  box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
  z-index: 1;
}

.dropdown-content a {
  color: black;
  padding: 12px 16px;
  text-decoration: none;
  display: block;
  text-align: left;
}

.dropdown-content a:hover {background-color: #f1f1f1}

.menu:hover .dropdown-content {
  display: block;
}
.social
{
   display: flex;
   flex-direction: column;
   position: fixed;
   z-index: 20;
   margin-top: 200px;
  
    width: 2px;

}
div .topnav
{position: fixed;}

.topnav {
  position: relative;

  
  background-color: #070607;
  height: 60px;
  padding: 10px;
  margin-top: 150px;
  border-radius: 10px;



}



 a {
 float: left;
  font-style: bold;
  padding: 20px  50px;
  text-decoration: none;
  font-size: 22px;
  font-family: Lato ;
     color: #ffd025;


}

div a:hover {
  opacity: 0.6;


}

.topnav a.active {
  background-color: #4CAF50;
  color: white ;
}

.topnav-centered .zink {
  float: none;
  position: absolute;
  left:52%;
  top: 52%;
  transform: translate(-50%, -50%);
  font-family:  Stencil ;
  font-size: 70px;
  color: #ffd025;
background-attachment: scroll;

}

.topnav-right {
  float: right;
}

/* Responsive navigation menu (for mobile devices) */
@media screen and (max-width: 600px) {
  .topnav a, .topnav-right {
    float: none;
    display: block;
  }

  .topnav-centered a {
    position: relative;
    top: 0;
    left: 0;
    transform: none;
  }
}




 

