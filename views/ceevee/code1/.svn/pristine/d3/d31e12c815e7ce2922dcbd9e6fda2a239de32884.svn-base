@font-face
{
 font-family:
  myfont;
  src: url(myfont.otf);
}
 a {
 float: left;
  font-style: bold;
  padding: 20px  50px;
  text-decoration: none;
  font-size: 22px;
  
     color: white;

}

div a:hover {
  opacity: 0.6;


}


/* Responsive navigation menu (for mobile devices) */
@media screen and (max-width: 600px)
 {
  .topnav a, .topnav-right {
    float: none;
    display: block;
  }

  .topnav-centered a {
    position: relative;
    top: 0;
    left: 0;
    font-size: 5px;
    transform: none;
  }
}

